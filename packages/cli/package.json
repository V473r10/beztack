{
  "name": "beztack",
  "version": "0.0.5",
  "description": "Beztack initialization and module management CLI",
  "type": "module",
  "license": "MIT",
  "bin": {
    "beztack": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "start": "pnpm run build && node dist/cli.js",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.1",
    "glob": "^11.0.0",
    "picocolors": "^1.1.1",
    "ts-morph": "^25.0.0"
  },
  "devDependencies": {
    "@nx/devkit": "21.4.1",
    "@types/node": "^22.15.17",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
