{
  "name": "@beztack/mercadopago",
  "version": "0.0.1",
  "description": "Mercado Pago SDK for Beztack - Server and React integrations",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepare": "pnpm build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "zod": "^3.23.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@mercadopago/sdk-react": "^1.0.7"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "@types/node": "^24.10.2",
    "@types/react": "^19.1.8",
    "react": "^19.0.0",
    "typescript": "^5.9.3",
    "zod": "^3.25.60"
  },
  "keywords": [
    "mercadopago",
    "payments",
    "subscriptions",
    "checkout",
    "react",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.12.1"
}
