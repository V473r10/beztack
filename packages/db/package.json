{
  "name": "@beztack/db",
  "version": "0.0.11",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./drizzle-config": {
      "types": "./drizzle.config.ts",
      "import": "./drizzle.config.ts"
    }
  },
  "scripts": {
    "migrate": "pnpm exec drizzle-kit generate --config=./drizzle.config.ts && pnpm exec drizzle-kit migrate --config=./drizzle.config.ts",
    "push": "pnpm exec drizzle-kit push --config=./drizzle.config.ts",
    "generate-auth-schema": "pnpm dlx @better-auth/cli generate --output ./src/auth-schema.generated.ts --config ../../apps/api/server/utils/auth.ts -y",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@beztack/env": "workspace:*",
    "drizzle-orm": "^0.44.7",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.9.3"
  }
}
