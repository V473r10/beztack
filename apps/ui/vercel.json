{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://acervus-api.vercel.app/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://acervus-ui.vercel.app"
        }
      ]
    }
  ]
}
